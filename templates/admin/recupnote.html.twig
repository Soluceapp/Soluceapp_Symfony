{% extends '@EasyAdmin/page/content.html.twig' %} 


{% block content %}
<head>
        <meta charset="UTF-8">
        <title>{% block title %}Mooc.soluceapp.com{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{asset('styles/app.css') }}">
            <link rel="icon" type="image/png" href="{{asset('images/favicon.png') }}">
            <link rel="shortcut icon" href="{{asset('images/favicon.ico') }}">
        {% endblock %}
</head>

<div style="margin:20px"><div class="style16"><br>
<h3>Récupération des notes</h3>
<br>

    {{ form_start(registrationForm, {'method': 'POST'}) }}

    De quelle classe voulez-vous les notes ?<br><br>
    {{ form_row(registrationForm.classe) }}
    {{ form_row(registrationForm.id_domain) }}<br>
    <button type="submit" class="btn btn-primary btn-lg">Valider</button><br><br>
   
    {{ form_end(registrationForm) }}



{% for dutil in dutil %}
    {% if  dutil.classe.id == Nameclasse and dutil.iddomain.id == Namedomaine %}
        <li>{{ dutil.Prenom }} &nbsp; {{ dutil.Nom }} : {{ dutil.Points }} points</li>
    {% endif %}
{% endfor %}
<br>
</div></div>

{% endblock %}